\ProvidesPackage{logoups}

\RequirePackage{helvet}
\RequirePackage{pgf}
\newdimen\logo@ups@xposbegin
\newdimen\logo@ups@xposend
\newdimen\logo@ups@ypos
\newdimen\logo@ups@longueur
\newbox\logo@ups@box

\setbox\logo@ups@box\hbox{{%
    \definecolor{rougeups}{cmyk}{.25,1,1,0}%
    \definecolor{jauneups}{cmyk}{0,.22,.75,0}%
    \definecolor{bleuups}{cmyk}{.58,.17,0,.46}%
    \begin{pgfpicture}%
      \let\familydefault\sfdefault
      \logo@ups@ypos=2cm
      \logo@ups@xposbegin=2cm
      \logo@ups@xposend=4.83cm
      % 
      \begin{pgfscope}% rectangle exterieur
        \color{rougeups}%
        \pgfpathrectangle{\pgfpointorigin}{\pgfpoint{\logo@ups@xposend}{\logo@ups@ypos}}
        \pgfsetlinewidth{1pt}
        \pgfusepath{stroke}
      \end{pgfscope}
      \begin{pgfscope}
        \color{rougeups}
        \pgfpathrectangle{\pgfpointorigin}{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@ypos}}
        \pgfusepath{fill}
      \end{pgfscope}
      \begin{pgfscope} % texte
        \addtolength\logo@ups@xposbegin{.9em} \addtolength\logo@ups@xposend{-.9em}
        \setlength\logo@ups@longueur{\logo@ups@xposend} \addtolength\logo@ups@longueur{-\logo@ups@xposbegin}
        \begin{pgfscope}
          \color{bleuups}
          \pgftext[left,base,at=\pgfpoint{\logo@ups@xposbegin}{1cm}]{\makebox[\logo@ups@longueur][s]{\bf Paul Sabatier}}
          \pgftext[left,top,at=\pgfpoint{\logo@ups@xposbegin}{.4cm}]{\makebox[\logo@ups@longueur][s]{\scriptsize\sc t o u l o u s e ~ i i i}}
          \pgftext[left,base,at=\pgfpoint{\logo@ups@xposbegin}{1.35cm}]{\makebox[\logo@ups@longueur]{\bf Universit{\'e}}}
          \pgfusepath{stroke}
        \end{pgfscope}
        \begin{pgfscope}
          \color{rougeups}
          \pgfsetlinewidth{0.5pt}
          \setlength\logo@ups@ypos{0.7cm}
          \pgfmoveto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@ypos}}
          \pgflineto{\pgfpoint{\logo@ups@xposend}{\logo@ups@ypos}}
          \pgfusepath{stroke}
        \end{pgfscope}
      \end{pgfscope}
      % logo S
      \begin{pgfscope}
        \pgfsetlinewidth{.8pt}
        \setlength{\logo@ups@longueur}{.1cm}
        \setlength\logo@ups@xposend{1cm}
        \setlength\logo@ups@xposbegin{1cm}
        \color{jauneups}
        % cercles exterieurs
        \addtolength{\logo@ups@xposend}{-\logo@ups@longueur}
        \pgfpathcircle{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin}}{\logo@ups@xposend}
        \addtolength{\logo@ups@xposend}{-\logo@ups@longueur}
        \pgfpathcircle{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin}}{\logo@ups@xposend}
        \addtolength{\logo@ups@xposend}{-\logo@ups@longueur}
        \pgfusepath{stroke}
        % cercle interieur, droite
        \begin{pgfscope}
          {%
            \pgfpathmoveto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin-\logo@ups@xposend}}
            \pgfpatharc{-90}{73.4}{\logo@ups@xposend}
            \pgflineto{\pgfpoint{\logo@ups@xposbegin+2\logo@ups@longueur}{\logo@ups@xposbegin+3\logo@ups@longueur}}
            \pgflineto{\pgfpoint{\logo@ups@xposbegin-1.5\logo@ups@longueur}{\logo@ups@xposbegin+3\logo@ups@longueur}}
            \pgfpatharc{90}{270}{\logo@ups@longueur}
            \pgflineto{\pgfpoint{\logo@ups@xposbegin+1.5\logo@ups@longueur}{\logo@ups@xposbegin+\logo@ups@longueur}}
            \pgfpatharc{90}{-90}{3\logo@ups@longueur}
            \pgflineto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin-5\logo@ups@longueur}}
            \pgflineto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin-7\logo@ups@longueur}}
            \pgfusepath{stroke}}
        \end{pgfscope}
        % cercle interieur, gauche
        \begin{pgfscope}
          \pgfpathmoveto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin+\logo@ups@xposend}}
          \pgfpatharc{90}{253.4}{\logo@ups@xposend}
          \pgflineto{\pgfpoint{\logo@ups@xposbegin-2\logo@ups@longueur}{\logo@ups@xposbegin-3\logo@ups@longueur}}
          \pgflineto{\pgfpoint{\logo@ups@xposbegin+1.5\logo@ups@longueur}{\logo@ups@xposbegin-3\logo@ups@longueur}}
          \pgfpatharc{-90}{90}{\logo@ups@longueur}
          \pgflineto{\pgfpoint{\logo@ups@xposbegin-1.5\logo@ups@longueur}{\logo@ups@xposbegin-\logo@ups@longueur}}
          \pgfpatharc{270}{90}{3\logo@ups@longueur}
          \pgflineto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin+5\logo@ups@longueur}}
          \pgflineto{\pgfpoint{\logo@ups@xposbegin}{\logo@ups@xposbegin+7\logo@ups@longueur}}
          \pgfusepath{stroke}
        \end{pgfscope}
        \begin{pgfscope}
        \end{pgfscope}
        \pgfmoveto{\pgfpoint{\logo@ups@xposbegin+\logo@ups@longueur}{\logo@ups@xposbegin+8\logo@ups@longueur-.3pt}} % sin(acos(\logo@ups@longueur/rayon))
        \pgflineto{\pgfpoint{\logo@ups@xposbegin+\logo@ups@longueur}{\logo@ups@xposbegin+4\logo@ups@longueur}}
        \pgflineto{\pgfpoint{\logo@ups@xposbegin-1.5\logo@ups@longueur}{\logo@ups@xposbegin+4\logo@ups@longueur}}
        \pgfpatharc{90}{270}{2\logo@ups@longueur}
        \pgflineto{\pgfpoint{\logo@ups@xposbegin+1.5\logo@ups@longueur}{\logo@ups@xposbegin}}
        \pgfpatharc{90}{-90}{2\logo@ups@longueur}
        \pgflineto{\pgfpoint{\logo@ups@xposbegin-\logo@ups@longueur}{\logo@ups@xposbegin-4\logo@ups@longueur}}
        \pgflineto{\pgfpoint{\logo@ups@xposbegin-\logo@ups@longueur}{\logo@ups@xposbegin-8\logo@ups@longueur+.3pt}}
        \pgfusepath{stroke}
      \end{pgfscope}
    \end{pgfpicture}}}

\def\logoups{\usebox\logo@ups@box}
